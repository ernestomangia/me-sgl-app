   <!-- Modal -->
<div class="modal fade modalEliminar" id="modalEliminarCobrador" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Eliminar Cobrador</h4>
            </div>
            <div class="modal-body">
                ¿Esta seguro que desea eliminar el Cobrador?
            </div>
            <div class="modal-footer">
                <div class="row text-left">
                    <div class="col-md-12">
                        <div class="alert alert-danger validationSummary hide" role="alert">
                            <a class="close" data-hide="alert" aria-hidden="true">×</a>
                            <b>Mensajes:</b>
                        </div>
                    </div>
                </div>
                <button type="button" class="btn btn-default btnCancelarEliminar" data-dismiss="modal">Cancelar</button>
                <button id="btnAceptar" type="button" class="btn btn-primary">Aceptar</button>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $("#btnAceptar").on("click", function () {
        var id = $("#idEntidad").val();
        var success = ('@Messages.EntidadEliminada').replace("{0}", '@Messages.ElCobrador').replace("{1}", id);
        eliminarCobrador("@Url.Action("Eliminar")", success, '@ErrorMessages.ErrorEliminar');
    });

    function eliminarCobrador(url, msjSuccess) {
        var id = $("#idEntidad").val();
        $(".modalAnular .validationSummary").addClass("hide");
        $(".modalAnular .validationSummary ul").remove();

        $.ajax({
            method: "GET",
            url: url + "?id=" + id,
            dataType: "json",
            error: function (jqXhr, status, error) {
                mensajeError("Ha ocurrido un error");
            },
            success: function (data) {
                if (data.Success) {
                    var dataTable = $(".dataTableCustom").DataTable();
                    $('.modalAnular').modal('toggle');
                    mensajeSuccess(msjSuccess);
                    console.log($("#tr_" + id + " .CambioEstado"));
                    dataTable.cell($("#tr_" + id), (6)).data('Anulado').draw();
                    $(".btnCancelarEliminar").click();
                } else {
                    var errores = "<ul>";
                    $.each(data.Errors, function (key, value) {
                        $.each(value.Value, function (key2, value2) {
                            errores += "<li>" + value2 + "</li>";
                        });
                    });
                    errores += "</ul>";
                    $(".modalAnular .validationSummary").append(errores);
                    $(".modalAnular .validationSummary").removeClass("hide");
                }
            },
            timeout: 10000,
            cache: false
        });
    }
</script>

