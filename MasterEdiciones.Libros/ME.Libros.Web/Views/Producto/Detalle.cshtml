@model ME.Libros.Web.Models.ProductoViewModel

<h3>Detalle producto</h3>
<div id="formProducto" class="form-horizontal">
    @Html.Partial("Shared/Form_Partial", Model)

    <div class="form-group">
        <div class="col-sm-offset-8">
            <a id="btnCancelarProducto" class="btn btn-default" href="@Url.Action("Index")">
                Cancelar
            </a>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $("#formProducto :input").each(function () {
            $(this).attr('disabled', true);
        });
    });
</script>


